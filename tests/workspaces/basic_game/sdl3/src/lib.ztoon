package SDL3
import core::core;
pub union event 
{
    type: event_type;
    key: keyboard_event;
    padding : u8[128];
}

pub struct window 
{
    padding: u8[448];
}
pub struct renderer 
{
    padding: u8[712];
    fn set_draw_color(self*, r: u8, g: u8, b: u8, a: u8) -> bool {
        ret SDL_SetRenderDrawColor(self, r, g, b, a) as bool;
    }

    fn clear(self*) -> bool
    {
        ret SDL_RenderClear(self) as bool;
    }
    
    fn present(self*) -> bool 
    {
        ret SDL_RenderPresent(self) as bool;
    }

    fn fill_rect(self*, r: rect<[f32]>*) -> bool 
    {
        ret SDL_RenderFillRect(self, r) as bool;
    }

    fn debug_text(self*, x: f32, y: f32, str: readonly i8*) -> bool
    {
        ret SDL_RenderDebugText(self, x, y, str) as bool;
    }

    fn set_draw_scale(self*, scale_x: f32, scale_y: f32) -> bool {
        ret SDL_SetRenderScale(self, scale_x, scale_y) as bool;
    }
}


pub struct rect<[T]>
{
    x: T;
    y: T;
    w: T;
    h: T; 
}
struct keyboard_event
{
    type: event_type;    /**< SDL_EVENT_KEY_DOWN or SDL_EVENT_KEY_UP */
    reserved : u32;
    timestamp: u64;       /**< In nanoseconds, populated using SDL_GetTicksNS() */
    windowID: u32;  /**< The window with keyboard focus, if any */
    which : u32;   /**< The keyboard instance id, or 0 if unknown or virtual */
    scancode: i32;  /**< SDL physical key code */
    code : key_code;        /**< SDL virtual key code */
    mod: u16;         /**< current key modifiers */
    raw: i16;             /**< The platform dependent scancode for this event */
    down: i32;              /**< true if the key is pressed */
    repeat: i32;            /**< true if this is a key repeat */
};

fn SDL_Init(flags: u32) -> i32;

pub fn init(flags: init_flags) -> bool
{
    ret SDL_Init(flags as u32) as bool;
}

fn SDL_GetError() -> readonly i8*;

pub fn get_error() -> readonly i8* 
{
    ret SDL_GetError();
}

fn SDL_CreateWindow(title: readonly i8*, w: i32, h: i32, flags: u64) -> window*;

pub fn create_window(title: readonly i8*, w: i32, h: i32, flags: window_flags) -> window*
{
    ret SDL_CreateWindow(title, w, h, flags as u64);
}

fn SDL_CreateRenderer(w: window*, name: readonly i8*) -> renderer*;

pub fn create_renderer(w: window*, name: readonly i8*) -> renderer*
{
    ret SDL_CreateRenderer(w, name);
}

fn SDL_CreateWindowAndRenderer(title: readonly i8*, width: i32, height: i32, flags: u64, w: window**, r: renderer**) -> i32;

pub fn create_window_and_renderer(title: readonly i8*, width: i32, height: i32, flags: window_flags, w: window**, r: renderer**) -> bool
{
    ret SDL_CreateWindowAndRenderer(title, width, height, flags as u64, w, r) as bool;
}

pub fn SDL_PollEvent(e: event*) -> i32;

pub fn poll_event(e: event*) -> bool
{
    ret SDL_PollEvent(e) as bool;
}

fn SDL_SetRenderDrawColor(_r: renderer*, r: u8, g: u8, b: u8, a: u8) -> i32;

fn SDL_RenderClear(r: renderer*) -> i32;

fn SDL_RenderPresent(r: renderer*) -> i32;

fn SDL_Delay (ms: u32);

pub fn delay(ms: u32) 
{
    SDL_Delay(ms);
}

fn SDL_RenderFillRect(r: renderer*, rect: rect<[f32]>*) -> i32;


fn SDL_rand(n: i32) -> i32;

pub fn rand(n: i32) -> i32 
{
    ret SDL_rand(n);
}


fn SDL_GetTicks() -> u64;

pub fn get_ticks() -> u64  
{
    ret SDL_GetTicks();
}


fn SDL_RenderDebugText(r: renderer*, x: f32, y: f32, str: readonly i8*) -> i32;

fn SDL_SetRenderScale(r: renderer*, scale_x: f32, scale_y: f32) -> i32;

pub fn SDL_snprintf(text : i8*, max_len: u64, fmt: readonly i8*, ...) -> i32;

pub enum init_flags : u32
{
    SDL_INIT_AUDIO = 0x00000010,
    SDL_INIT_VIDEO = 0x00000020,
    SDL_INIT_JOYSTICK = 0x00000200,
    SDL_INIT_HAPTIC = 0x00001000,
    SDL_INIT_GAMEPAD = 0x00002000,
    SDL_INIT_EVENTS = 0x00004000,
    SDL_INIT_SENSOR = 0x00008000,
    SDL_INIT_CAMERA = 0x00010000,
    SDL_INIT_EVERYTHING = SDL_INIT_AUDIO | SDL_INIT_VIDEO | SDL_INIT_JOYSTICK |
                          SDL_INIT_HAPTIC | SDL_INIT_GAMEPAD| SDL_INIT_EVENTS |
                          SDL_INIT_SENSOR | SDL_INIT_CAMERA,

}
