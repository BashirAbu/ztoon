
package mainPkg;

fn sqrt(value: f64) -> f64;
fn pow(value: f64, power: f64) -> f64;
fn printf(str: readonly i8*, ...);

fn test()
{
    printf("hi there");
}

struct Vector3
{
    x: f64;
    y: f64;
    z: f64;
    fn test(self*)
    {
        self.x = 12.0343;
    }
    fn GetLen(self*) -> f64
    {
        ret sqrt(pow(self.x, 2.0) + pow(self.y, 2.0) + pow(self.z, 2.0));
    }
}


fn main() -> i32
{
    test();
    position: Vector3 = {12.0, 1.0, 99.0};
    len: f64 = position.GetLen();
    position.test();
    printf("%f\n", position.x);
    // length: f64 = Vector3::GetLen(&position);
    printf("Len: %f\n", len);
    // printf("Length: %f\n", length);
    ret 0;
}
